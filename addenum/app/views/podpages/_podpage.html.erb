<div class="col s12 m6">
  <div class="card blue-grey darken-1">
    <div class="card-content white-text">
      <%=podpage.name%>
      <%=podpage.description%>
      <%=podpage.id %>
      <%= link_to podpage, remote: true, method: :delete do %>
      <button>Delete!</button>
      <% end %>
      <%= link_to podpage_path(podpage.id) do %>
      <button>CLICK to see podpage!</button>
      <% end %>
    </div>
  </div>
</div>
